   "source": [
    "import pyttsx3\n",
    "text = '''\n",
    "\n",
    "blah blah blah\n",
    "\n",
    "'''\n",
    "text = text.replace('\\n', ' ')\n",
    "engine = pyttsx3.init()\n",
    "voices = engine.getProperty('voices')\n",
    "engine.setProperty('voice', voices[1].id)\n",
    "engine.setProperty('rate', 200)\n",
    "engine.say(text)\n",
    "engine.runAndWait()"
   ]
